{
  "manifest_version": 2,
  "name": "eMAL",
  "version": "0.2",

  "description": "Quality-of-life enhancements for MyAnimeList",

  "icons": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://myanimelist.net/animelist/*"],
        "js": ["eMAL.js"],
        "css": ["eMAL.css"]
    }
  ]

}
